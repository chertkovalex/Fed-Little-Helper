{
    "name": "FedLittleHelper",
    "version": "0.1",
    "description": "A bare-bones example of communication between devtools and an inspected page",
    "devtools_page": "devtools.html",
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "browser_action": {
      "default_icon": "SantaLittleHelper.png"
    },
    "permissions": [
        
        "tabs", 
        "webRequest",
        "http://*/*",
        "https://*/*"
    ],
    "manifest_version": 2
}